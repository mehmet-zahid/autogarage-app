{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "main-capability",
    "description": "Capability for the main window",
    "windows": ["main"],
    "permissions": [
      "sql:allow-load",
      "sql:allow-close",
      "sql:allow-execute",
      "sql:allow-select",
      "path:default",
      "event:default",
      "window:default",
      "app:default",
      "resources:default",
      "menu:default",
      "tray:default",
      "window:allow-set-title"
    ]
  }